{
  "name": "gas",
  "description": "gas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "login": "clasp login",
    "create": "clasp create --auth ./.clasprc.json --type standalone --rootDir ./src --title $PROJECT",
    "open": "clasp open --auth ./.clasprc.json",
    "pull": "clasp pull --auth ./.clasprc.json",
    "push": "clasp push --auth ./.clasprc.json",
    "watch": "clasp push --auth ./.clasprc.json --watch",
    "deployments": "clasp deployments --auth ./.clasprc.json",
    "deploy": "clasp deploy --auth ./.clasprc.json --deploymentId $(clasp deployments --auth ./.clasprc.json | awk '/- [0-9a-zA-Z-_]+ @[0-9]+/ { print $2; }')"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/clasp": "^2.4.2"
  },
  "devDependencies": {
    "@types/google-apps-script": "^1.0.59",
    "@types/google-apps-script-oauth2": "^38.0.0",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "node-fetch": "2.6.6",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
